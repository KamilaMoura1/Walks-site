<tr id="walkItem-<%= item.id %>" class="walkItem">
  <td>
    <span class="custom-checkbox">
      <input type="checkbox" name="options[]" value="1" />
      <label for="checkbox1"></label>
    </span>
  </td>
  <td><%= item.id %></td>
  <td class="walkItemName"><%= item.walk_title %></td>
  <td class="walkItemDescription"><%= item.description %></td>
  <td class="walkItemDate"><%= item.walk_date %></td>
  <td class="walkItemImage"><img src="<%= item.image_url %>" /></td>
  <td><%= item.created_at %></td>
  <td><%= item.updated_at %></td>

  <td>
    <a href="#editEmployeeModal" class="edit" data-toggle="modal"
      ><i class="material-icons" data-toggle="tooltip" title="Edit"
        >&#xE254;</i
      ></a
    >
    <a href="#deleteEmployeeModal" class="delete" data-toggle="modal"
      ><i class="material-icons" data-toggle="tooltip" title="Delete"
        >&#xE872;</i
      ></a
    >
  </td>
</tr>
